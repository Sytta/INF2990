{
  "name": "sudoku-client",
  "version": "1.0.0",
  "description": "client de base pour le projet 2/Sudoku",
  "scripts": {
    "start": "npm run-script copy-assets && tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lint": "tslint app/**/*.ts app/*.ts --force -t verbose",
    "test": "npm run-script copy-assets && tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "npm run-script copy-assets && tsc && karma start karma.conf.js --single-run || npm run-script coverage",
    "coverage": "remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf out node_modules",
    "postinstall": "rimraf false",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "copy-assets": "gulp assets"
  },
  "dependencies": {
    "@angular/animations": "4.0.1",
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/flex-layout": "2.0.0-beta.7",
    "@angular/forms": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "@angular/router": "4.0.1",
    "angular-in-memory-web-api": "0.3.1",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "rxjs": "5.3.0",
    "socket.io": "1.7.3",
    "systemjs": "0.20.12",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/hammerjs": "2.0.34",
    "@types/mocha": "2.2.40",
    "@types/node": "7.0.12",
    "@types/sinon": "2.1.2",
    "@types/sinon-chai": "2.7.27",
    "canonical-path": "0.0.2",
    "chai": "3.5.0",
    "codelyzer": "2.1.1",
    "concurrently": "3.4.0",
    "gulp": "3.9.1",
    "http-server": "0.9.0",
    "istanbul": "0.4.5",
    "karma": "1.6.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.3",
    "lite-server": "2.3.0",
    "lodash": "4.17.4",
    "mocha": "3.2.0",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.1",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "tslint": "4.5.1",
    "tslint-eslint-rules": "3.5.1",
    "typescript": "2.2.2"
  }
}
